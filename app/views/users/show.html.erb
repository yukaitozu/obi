<div class = "container">
  <div class =" profile-info">
    <div>
      <% if @user.photo.attached? %>
        <%= cl_image_tag @user.photo.key, height: 150, width: 200, crop: :fill, class: "avatar-round-large" %>
      <% else %>
        <%= cl_image_tag("blank_avatar_copy_c1izso", height: 150, width: 200, crop: :fill, class: "avatar-round-large") %>
      <% end %>
    </div>
    <h1><%= @user.first_name  %></h1>
    <h1><%= @user.last_name %></h1>
    <h1><%= @user.profile_info %></h1>
  </div>
</div>
